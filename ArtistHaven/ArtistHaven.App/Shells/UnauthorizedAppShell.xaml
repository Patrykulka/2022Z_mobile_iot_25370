<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ArtistHaven.App.Shells.UnauthorizedAppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ArtistHaven.App"
    xmlns:pages="clr-namespace:ArtistHaven.App.Pages"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab Title="Login">
            <ShellContent ContentTemplate="{DataTemplate pages:LoginPage}"/>
        </Tab>
        <Tab Title="Register">
            <ShellContent ContentTemplate="{DataTemplate pages:RegisterPage}"/>
        </Tab>
    </TabBar>

</Shell>
